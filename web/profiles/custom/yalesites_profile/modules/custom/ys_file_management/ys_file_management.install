<?php

/**
 * @file
 * Install, update and uninstall functions for the ys_file_management module.
 */

/**
 * Implements hook_install().
 */
function ys_file_management_install() {
  // Set module weight higher than media_file_delete to ensure our
  // hook_entity_type_alter() runs after theirs.
  module_set_weight('ys_file_management', 10);
}
