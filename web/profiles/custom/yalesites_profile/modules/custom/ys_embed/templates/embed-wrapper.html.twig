{% if display_attributes['isIframe'] %}
  <iframe data-embed-type="{{ embed_type }}" src="{{ url }}" title="{{ title }}" height="{{ display_attributes['height'] }}" width="{{ display_attributes['width'] }} scrolling="{{ display_attributes['scrolling'] }}" frameborder="{{ display_attributes['frameborder'] }}"></iframe>   
{% else %}
  {{ embedSource }}
{% endif %}
