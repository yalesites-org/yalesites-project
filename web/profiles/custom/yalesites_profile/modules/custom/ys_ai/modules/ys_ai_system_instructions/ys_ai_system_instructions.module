<?php

/**
 * @file
 * YS AI System Instructions module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ys_ai_system_instructions_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.ys_ai_system_instructions':
      return '<p>' . t('This module provides system instructions management for YaleSites AI with versioning and API synchronization capabilities.') . '</p>';
  }
}
