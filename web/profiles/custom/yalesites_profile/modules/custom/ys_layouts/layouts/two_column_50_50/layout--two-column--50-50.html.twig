{#
/**
 * @file
 * Default theme implementation to display a one-column layout.
 *
 * Available variables:
 * - in_preview: Whether the plugin is being rendered in preview mode.
 * - content: The content for this layout.
 * - attributes: HTML attributes for the layout <div>.
 *
 * @ingroup themeable
 */
#}
{{ kint(settings.divider) }}
{% embed "@organisms/layout/layout/yds-layout.twig" with {
  layout__divider: settings.divider,
  component__theme: 'one',
} %}
  {% block layout__primary %}
    <div {{ region_attributes.content_primary }}>
      {{ content.content_primary }}
    </div>
  {% endblock %}
  {% block layout__secondary %}
    <div {{ region_attributes.content_secondary }}>
      {{ content.content_secondary }}
    </div>
  {% endblock %}
{% endembed %}
